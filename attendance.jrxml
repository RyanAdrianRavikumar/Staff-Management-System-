	<queryString language="SQL">
		<![CDATA[SELECT 
    a.attendance_id,
    s.first_name,
    s.last_name,
    a.checkin_time,
    a.checkout_time,
    TIMESTAMPDIFF(HOUR, a.checkin_time, a.checkout_time) AS total_hours_worked,
    TIMESTAMPDIFF(HOUR, a.checkin_time, a.checkout_time) - 8 AS overtime_hours,
    a.date
FROM 
    attendance a
JOIN 
    staff s ON a.staff_id = s.staff_id;]]>
	</queryString>
